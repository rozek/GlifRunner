  "bugs": {
    "url": "https://github.com/rozek/GlifRunner/issues"
  },
  "homepage": "https://github.com/rozek/GlifRunner#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^8.5.0",
    "agadoo": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.1",
    "tslib": "^2.6.2",
    "typescript": "^4.3.3"
  },
  "dependencies": {
    "javascript-interface-library": "^1.0.8"
  }
